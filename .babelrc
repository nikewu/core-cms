{
    "presets": [
      ["es2015", { "modules": false }],
      "stage-2"
    ],
    "plugins": [
      ["transform-runtime", {
        "helpers": true,
        "polyfill": false,
        "regenerator": false,
        "moduleName": "babel-runtime"
      }]
    ],
    "comments": false,
    "env": {
      "test": {
        "presets": ["env", "stage-2"],
        "plugins": ["istanbul"]
      }
    }
  }